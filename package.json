{
  "name": "dynapi",
  "version": "0.3.7",
  "description": "A dynamic routes rendering middleware for Express/Connect",
  "main": "index.js",
  "repository": "git@github.com:shirohana/dynapi.git",
  "author": "Hana Shiro <shirohana0608@gmail.com>",
  "license": "MIT",
  "keywords": [
    "http",
    "middleware",
    "api",
    "connect",
    "express",
    "server",
    "web",
    "framework"
  ],
  "files": [
    "dist/"
  ],
  "nyc": {
    "include": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run lint && cross-env NODE_ENV=test npm run build && cross-env DEBUG=api:* nyc ava --verbose test/ && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js lib test/*.js",
    "build": "rollup -c build/rollup.config.js",
    "watch": "npm run build -- -w",
    "prepare": "npm run build",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.1.0",
    "chokidar": "^2.0.0",
    "debug": "^3.0.1",
    "glob": "^7.1.2",
    "native-require": "^1.1.2",
    "source-map-support": "^0.5.0",
    "tappable": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "codecov": "^3.0.0",
    "connect": "^3.6.3",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.4",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "nyc": "^11.1.0",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-watch": "^4.3.1",
    "supertest": "^3.0.0"
  }
}
